  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.15",
    "@tailwindcss/postcss": "^4.1.15",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.45",
    "tailwindcss": "^4.1.0"
  }
}
